{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "api/stripe-webhook.js": {
      "runtime": "nodejs18.x"
    },
    "api/checkout.js": {
      "runtime": "nodejs18.x"
    }
  }
}
